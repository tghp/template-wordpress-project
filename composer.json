{
    "name": "tghp/$tghp:lowerCaseHyphenated$-project",
    "type": "project",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "kinsta/kinsta-mu-plugins",
                "type": "wordpress-muplugin",
                "version": "3.0.0",
                "dist": {
                    "url": "https://kinsta.com/kinsta-tools/kinsta-mu-plugins.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "composer",
            "url": "https://packagist.org"
        },
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        },
        {
            "type": "composer",
            "url": "https://packages.metabox.io/<API_KEY>"
        }
    ],
    "require": {
        "johnpbloch/wordpress": "^6",
        "tghp/wp-composer-clean": "^2.1",
        "vlucas/phpdotenv": "^5",
        "kinsta/kinsta-mu-plugins": "^3.0",
        "wpackagist-plugin/meta-box": "^5",
        "wpackagist-plugin/wordfence": "^7",
        "wpackagist-plugin/sar-friendly-smtp": "^1",
        "wpackagist-plugin/svg-support": "^2.3",
        "meta-box/meta-box-aio": "dev-master",
        "monolog/monolog": "^2"
    },
    "config": {
        "vendor-dir": "wp-content/vendor"
    },
    "extra": {
        "wordpress-install-dir": "wp",
        "installer-paths": {
            "wp-content/plugins/{$name}/": [
                "type:wordpress-plugin"
            ],
            "wp-content/mu-plugins/": [
                "type:wordpress-muplugin"
            ],
            "wp-content/themes/{$name}/": [
                "type:wordpress-theme"
            ]
        }
    }
}

